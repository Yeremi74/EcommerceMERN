{
    "version" : 2,
    "builds" : [
        {"src": "*.js, *.jsx", "use": "@vercel/node"}
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
             }
        }
    ]
}